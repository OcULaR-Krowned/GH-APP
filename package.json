{
  "name": "gamehubapp",
  "version": "1.0.806",
  "description": "",
  "main": "Client.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.3.1",
    "electron-updater": "^4.2.5",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "socket.io": "^2.3.0"
